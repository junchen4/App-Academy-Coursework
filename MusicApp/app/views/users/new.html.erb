<h1> Sign up! </h1>

<ul>
  <% if flash[:errors] %>
    <% flash[:errors].each do |error| %>
      <li> <%= error %> </li>
    <% end %>
  <% end %>
</ul>

<form action="<%=users_url%>" method="post">

  <label for="Email"> Email </label>
  <input id="Email"type="text" name="user[email]" value="">

  <br>

  <label for="Password"> Password </label>
  <input id="Password" type="text" name="user[password]" value="">
  <input type="Submit">

  <br>
  
</form>
